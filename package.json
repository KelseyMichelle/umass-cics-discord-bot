{
  "name": "umass-cics-discord-bot",
  "version": "2.0.0",
  "description": "Bot for unofficial UMass CICS discord.",
  "main": "./out/index.js",
  "type": "module",
  "scripts": {
    "build": "npx ttsc -b",
    "build-watch": "npx ttsc -b -w",
    "clean": "rm -rf ./out",
    "lint": "eslint ./src/**/*.ts",
    "start": "node --es-module-specifier-resolution=node --harmony ./out/index.js",
    "clean-start": "npm run clean && npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-melanson/UMass-CICS-Discord-Bot.git"
  },
  "author": "Daniel Melanson",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/daniel-melanson/UMass-CICS-Discord-Bot/issues"
  },
  "homepage": "https://github.com/daniel-melanson/UMass-CICS-Discord-Bot#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/mongodb": "^3.6.11",
    "@types/node": "^14.14.37",
    "@types/node-cron": "^2.0.3",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^2.2.2"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "discord.js": "^12.5.2",
    "dotenv": "^8.2.0",
    "mongodb": "^3.6.5",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1"
  },
  "engines": {
    "node": ">=13.0.0"
  }
}
